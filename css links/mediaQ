    /* Navigation Settings*/


ul {
  list-style: none;
  margin: 0;
  padding: 0;
    text-shadow: 1px 1px 17px #ffffff;
        
}


ul li {
  display: block;
  position: relative;
  float: left;
    font-size: 1.0em;
}



li ul { display: none; }

ul li a {
  display: block;
  text-decoration: none;
  white-space: nowrap;
  color: #fff;
}
ul li a:hover { background: black; }


li:hover > ul {
  display: block;
  position: absolute;
}

li:hover li { float: none; }

li:hover a { background: black; }

li:hover li a:hover { background: black; }

.main-navigation li ul li { border-top: 0; }


ul ul ul {
  left: 100%;
  top: 0;
}


ul:before,
ul:after {
  content: " "; /* 1 */
  display: table; /* 2 */
}

ul:after { clear: both; }



li {
  float: left;
  display: inline-block;
    padding: 5px;
}



li a {
  display: block;
  color: white;
  text-align: center;
  padding: 2px 33px;
  text-decoration: none;
    font-family:sans-serif;
    font-size: 1.97em;
    padding-top: 27px;
  
}



